{
  "extends": "airbnb",
  "parserOptions": {
    "ecmaVersion": 8
  },
  "env": {
    "node": true,
    "browser": true,
    "es6": true
  },
  "plugins": ["react", "jsx-a11y", "import"],
  "rules": {
    "no-console": "off",
    "no-unused-vars": [
      "error",
      {
        "args": "none"
      }
    ],
    "no-param-reassign": [
      "error",
      {
        "props": false
      }
    ],
    "no-underscore-dangle": "off"
  }
}
